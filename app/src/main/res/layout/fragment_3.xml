<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Fragment2"
    tools:ignore="MissingConstraints">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="257dp"
        android:layout_height="48dp"
        android:layout_below="@+id/textView5"
        android:layout_toStartOf="@+id/button1"
        android:layout_toLeftOf="@+id/button1"
        android:background="@drawable/login_design"
        app:layout_constraintBottom_toTopOf="@+id/spinner1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.837"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner2"
        app:layout_constraintVertical_bias="0.489"
        tools:ignore="UnknownIdInLayout" />

    <Spinner
        android:id="@+id/spinner1"
        android:layout_width="257dp"
        android:layout_height="48dp"
        android:background="@drawable/login_design"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.292"
        tools:layout_editor_absoluteX="129dp" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="257dp"
        android:layout_height="48dp"
        android:layout_below="@+id/spinner1"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/login_design"
        tools:layout_editor_absoluteX="129dp"
        tools:layout_editor_absoluteY="44dp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="86sp"
        android:layout_height="25sp"
        android:layout_alignParentTop="true"
        android:gravity="center"
        android:shadowColor="@color/black"
        android:shadowDx="1.0"
        android:shadowDy="1.0"
        android:shadowRadius="2.0"
        android:text="나이"
        android:textColor="#353535"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/spinner2"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.079" />

    <TextView
        android:id="@+id/textView80"
        android:layout_width="86sp"
        android:layout_height="25sp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/spinner1"
        android:layout_toLeftOf="@+id/spinner1"
        android:gravity="center"
        android:shadowColor="@color/black"
        android:shadowDx="1.0"
        android:shadowDy="1.0"
        android:shadowRadius="2.0"
        android:text="키"
        android:textColor="#353535"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView81"
        app:layout_constraintEnd_toStartOf="@+id/textView12"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView1"
        app:layout_constraintVertical_bias="0.495" />

    <TextView
        android:id="@+id/textView81"
        android:layout_width="86sp"
        android:layout_height="25sp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/spinner2"
        android:layout_toLeftOf="@+id/spinner2"
        android:gravity="center"
        android:shadowColor="@color/black"
        android:shadowDx="1.0"
        android:shadowDy="1.0"
        android:shadowRadius="2.0"
        android:text="몸무게"
        android:textColor="#353535"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/spinner1"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.293" />

    <EditText
        android:id="@+id/editText1"
        android:layout_width="257dp"
        android:layout_height="48dp"
        android:layout_alignStart="@+id/textView12"
        android:layout_alignLeft="@+id/textView12"
        android:layout_alignEnd="@+id/textView12"
        android:layout_alignRight="@+id/textView12"
        android:layout_alignBottom="@+id/spinner1"
        android:layout_alignParentTop="true"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/spinner2"
        app:layout_constraintEnd_toEndOf="@+id/spinner2"
        app:layout_constraintStart_toStartOf="@+id/spinner2"
        app:layout_constraintTop_toTopOf="@+id/spinner2" />

    <Button
        android:id="@+id/button2"
        android:layout_width="343dp"
        android:layout_height="48dp"
        android:layout_below="@+id/spinner2"
        android:backgroundTint="@color/base"
        android:text="다음"
        android:textColor="@color/white"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.415" />

</androidx.constraintlayout.widget.ConstraintLayout>